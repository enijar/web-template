{
  "type": "module",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development conc -n client,server,emails \"npm run start:client\" \"npm run start:server\" \"npm run start:emails\"",
    "build": "NODE_ENV=production npm run build:client && npm run build:server",
    "check": "tsc --noEmit",
    "test": "NODE_ENV=test exit 0",
    "format": "prettier --write src",
    "format:emails": "prettier --write src/emails",
    "format:client": "prettier --write src/client",
    "format:server": "prettier --write src/server",
    "start:emails": "email dev -d src/emails -p 9999",
    "start:client": "vite",
    "start:server": "tsx --watch src/server/index.ts",
    "build:client": "vite build",
    "build:server": "rm -rf build/{config,server} && tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json"
  },
  "dependencies": {
    "@hono/node-server": "1.19.7",
    "@hono/trpc-server": "0.4.1",
    "@react-email/components": "1.0.2",
    "@sendgrid/mail": "8.1.6",
    "@sequelize/core": "7.0.0-alpha.47",
    "@sequelize/mysql": "7.0.0-alpha.47",
    "@sequelize/sqlite3": "7.0.0-alpha.47",
    "@tanstack/react-query": "5.90.12",
    "@trpc/client": "11.8.1",
    "@trpc/react-query": "11.8.1",
    "@trpc/server": "11.8.1",
    "argon2": "0.44.0",
    "dotenv": "17.2.3",
    "hono": "4.11.3",
    "jose": "6.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "7.11.0",
    "styled-components": "6.1.19",
    "zod": "4.2.1",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@react-email/preview-server": "5.1.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-styled-components": "2.1.4",
    "concurrently": "9.2.1",
    "prettier": "3.7.4",
    "react-email": "5.1.0",
    "tsc-alias": "1.8.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3"
  }
}
